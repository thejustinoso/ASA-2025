;
; BIND data file for siteB.local zone
;
$TTL	604800
@	IN	SOA	dns-server. siteB.local. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@ IN NS dns-server.

dns-server. IN A 172.20.0.10
www.siteB.local. IN A 172.20.0.13
api.siteB.local. IN A 172.20.0.14